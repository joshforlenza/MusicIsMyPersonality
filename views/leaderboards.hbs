<table id="example" class="table table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Name</th>
                <th>Obscurity</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td><a class="profileLink" href="/profile/{{this.slug}}">{{this.username}}</a></td>
                <td>{{this.stats.obscurity}}</td>
            </tr>
            {{/each}}
            
        </tbody>
    </table>